{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://betthat-api.azurewebsites.net/api/*"
    },
    {
      "route": "/ws/*",
      "rewrite": "https://betthat-api.azurewebsites.net/ws/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/ws/*"]
  }
}
